# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

def base_pods
  use_modular_headers!
  pod 'ObjectMapper'
  pod 'Alamofire'
  pod 'AlamofireObjectMapper'
  pod 'PINRemoteImage'
  pod 'SwiftMessages'
end

target 'ArchitectureDemonstration' do
  # Comment the next line if you don't want to use dynamic frameworks
  base_pods
end

target 'ArchitectureDemonstrationTests' do
  base_pods
end
